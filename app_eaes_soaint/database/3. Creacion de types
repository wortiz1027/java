CREATE OR REPLACE TYPE entrevista_type AS OBJECT
(
    ID_ENTREVISTA        NUMBER,
    ID_CURRICULUM        NUMBER,
    ID_COLABORADOR       NUMBER,
    ID_VACANTE           NUMBER,
    fecha_realizacion    DATE,
    RESULTADO            VARCHAR2(20),
    FECHA_EMISION_RESULTADO DATE,
    OBSERVACIONES        VARCHAR2(100),
    CONSTRUCTOR FUNCTION entrevista_type(ID_ENTREVISTA NUMBER,
        ID_CURRICULUM NUMBER, ID_COLABORADOR NUMBER, ID_VACANTE NUMBER, fecha_realizacion DATE, RESULTADO VARCHAR2, FECHA_EMISION_RESULTADO DATE, OBSERVACIONES VARCHAR2 )
    RETURN SELF AS RESULT
);
/

CREATE OR REPLACE TYPE BODY entrevista_type IS   
    CONSTRUCTOR FUNCTION entrevista_type(ID_ENTREVISTA NUMBER,
    ID_CURRICULUM NUMBER, ID_COLABORADOR NUMBER, ID_VACANTE NUMBER, fecha_realizacion DATE, RESULTADO VARCHAR2, FECHA_EMISION_RESULTADO DATE, OBSERVACIONES VARCHAR2 ) 
                               RETURN SELF AS RESULT IS
    BEGIN
        SELF.ID_ENTREVISTA := ID_ENTREVISTA;
        SELF.ID_CURRICULUM := ID_CURRICULUM;
        SELF.ID_COLABORADOR := ID_COLABORADOR;
        SELF.ID_VACANTE := ID_VACANTE;
        SELF.RESULTADO := RESULTADO;
        SELF.FECHA_EMISION_RESULTADO := FECHA_EMISION_RESULTADO;
        SELF.OBSERVACIONES := OBSERVACIONES;
        RETURN;
    END;
END; 
/
COMMIT;

CREATE OR REPLACE TYPE array_Entrevista_Type IS TABLE OF entrevista_type;
COMMIT;